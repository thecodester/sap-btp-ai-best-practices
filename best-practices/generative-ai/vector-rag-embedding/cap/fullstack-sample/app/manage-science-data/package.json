{
  "name": "ui5.manage.sciencedata",
  "version": "1.0.0",
  "description": "Showcase of a setup for developing UI5 applications",
  "author": "SAP SE",
  "license": "Apache-2.0",
  "scripts": {
    "build": "ui5 build --clean-dest --dest ../router/dist",
    "build:opt": "ui5 build self-contained --clean-dest --all",
