    "start:dist": "ui5 serve  --port 8080 -o index.html --config ui5-dist.yaml",
    "lint": "eslint webapp"
  },
  "devDependencies": {
    "@ui5/cli": "^4",
    "@ui5/linter": "^1.5.0",
    "babel-plugin-istanbul": "^7.0.0",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "ui5-middleware-livereload": "^3",
    "ui5-middleware-simpleproxy": "^3",
    "ui5-test-runner": "^5.4.3"
  }
}